{
  "name": "create-stack-genius",
  "version": "1.0.2",
  "description": "A CLI tool to create projects with Express, Hono, and Prisma",
  "bin": {
    "create-stack-genius": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r 'src/templates/' dist/templates/",
    "start": "node dist/cli.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,js}'"
  },
  "keywords": [
    "express",
    "hono",
    "prisma",
    "cli",
    "project generator",
    "template",
    "templates",
    "stack",
    "stack genius"
  ],
  "author": "Dev Rio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-rio/create-stack-genius.git"
  },
  "bugs": {
    "url": "https://github.com/dev-rio/create-stack-genius/issues"
  },
  "homepage": "https://github.com/dev-rio/create-stack-genius#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.0",
    "inquirer": "^8.2.5",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "tsx": "^4.7.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "type": "module"
}
