{
  "name": "create-stack-craft",
  "version": "1.1.0",
  "description": "A CLI tool to create projects with Express, Hono, and Prisma",
  "bin": {
    "create-stack-craft": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r 'src/templates/' dist/templates/",
    "start": "node dist/cli.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,js}' 'src/*.{ts,js,tsx}'"
  },
  "keywords": [
    "express",
    "hono",
    "prisma",
    "cli",
    "project generator",
    "template",
    "templates",
    "stack",
    "stack craft"
  ],
  "author": "Dev Rio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-rio/create-stack-craft.git"
  },
  "bugs": {
    "url": "https://github.com/dev-rio/create-stack-craft/issues"
  },
  "homepage": "https://github.com/dev-rio/create-stack-craft#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "path": "^0.12.7",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.19.42",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "type": "module"
}
